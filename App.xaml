<Application
    x:Class="SPTC_APP.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:SPTC_APP"
    StartupUri="View/Configuration/SplashScreen.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="View/Styles/Windows.xaml" />
                <ResourceDictionary Source="View/Styles/ButtonsAndTextBox.xaml" />
                <ResourceDictionary Source="View/Images/vector/Icons.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
