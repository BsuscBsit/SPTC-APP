<Window x:Class="SPTC_APP.View.LogsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SPTC_APP.View"
        mc:Ignorable="d"
        Title="LogsWindow" Height="450" Width="800">
    <Grid>
        <Grid Margin="50">
            <DataGrid x:Name="dgLogs"/>
        </Grid>
        <Button x:Name="btnReload" Content="RELOAD" HorizontalAlignment="Left" Margin="50,20,0,0" VerticalAlignment="Top" Click="btnReload_Click"/>

    </Grid>
</Window>
