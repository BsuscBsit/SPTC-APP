<Window x:Class="SPTC_APP.View.Pages.Output.Receipts"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SPTC_APP.View.Pages.Output"
        Focusable="False"
        IsHitTestVisible="False"
        IsTabStop="False"
        ResizeMode="NoResize"
        SizeToContent="WidthAndHeight"
        Topmost="False"
        WindowStyle="None"
        mc:Ignorable="d"
        Title="Receipts">
    <Grid x:Name="receiptpage" Height="400" Width="600">
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Label x:Name="lblTitle" Content="SPTC RECEIPT: " Grid.Row="0" HorizontalAlignment="Left" Margin="30,0,0,0" VerticalAlignment="Center" FontSize="15" Style="{StaticResource CommonTitlePreset}"/>
    </Grid>
</Window>
